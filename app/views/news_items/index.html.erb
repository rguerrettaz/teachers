<h1>Tumbles</h1>


  <% @tumbles.each do |post| %>
    <article class = 'news_item'>
      <ul>
        <li><%= link_to post['blog_name'], post['post_url'] %><li>
        <li class='popularity'><%= post['popularity'] %></li>
      </ul>
      <%= render post['type'], :post => post %>
      <%= render 'meta_info', :post => post %>
    </article>
  <% end %>



